{
  "name": "friendly-city-print-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./scripts/dev.js",
    "build": "next build",
    "start": "node ./scripts/start.js",
    "lint": "next lint",
    "format": "prettier --write .",
    "seed": "node ./scripts/seed.js",
    "test-setup": "node ./scripts/test-setup.js",
    "setup": "npm ci && npm run lint || true",
    "prepare": "node ../scripts/install-husky.js",
    "install-hooks": "git config core.hooksPath .githooks",
    "secret-check": "node ./scripts/check-secrets.js",
    "e2e:install": "npx playwright install",
    "docs:generate": "node ../scripts/gen-docs.js",
    "docs:check": "node ../scripts/check-docs.js",
    "rotate-keys": "../scripts/rotate-supabase-keys.sh",
    "e2e:seed": "node ./scripts/e2e-setup.js",
    "e2e:cleanup": "node ./scripts/e2e-cleanup.js",
    "e2e:run-prod": "npm run e2e:seed && npm run build && node ./scripts/e2e-run-prod.js",
    "clean:win": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-win.ps1",
    "setup-powershell": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/setup-powershell-env.ps1",
    "test:e2e": "node node_modules/@playwright/test/cli.js test --reporter=html",
    "test:e2e:ci": "cross-env PW_NO_WEB_SERVER=true npm run test:e2e"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.32.1",
    "next": "14.2.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^12.10.0"
  },
  "devDependencies": {
    "@types/react": "19.2.5",
    "@types/node": "20.8.3",
    "autoprefixer": "^10.4.14",
    "@playwright/test": "^1.43.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^8.44.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.21",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "husky": "^8.0.0",
    "typescript": "^5.2.2",
    "wait-on": "^7.0.1",
    "start-server-and-test": "^1.14.0"
  },
  "overrides": {
    "rimraf": "4.4.1",
    "glob": "9.3.1"
  }
}